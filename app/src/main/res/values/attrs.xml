<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="text" format="string" />
    <attr name="hint" format="string" />
    <attr name="isArrowVisible" format="boolean" />
    <attr name="isDelimiterVisible" format="boolean" />
    <attr name="icon" format="reference" />
    <attr name="iconBackground" format="color" />
    <attr name="iconBackgroundStrokeColor" format="color" />
    <attr name="editSearchViewBackgroundColor" format="color" />
    <attr name="editSearchViewBackgroundStrokeColor" format="color" />

    <declare-styleable name="IconButton">
        <attr name="icon" />
        <attr name="iconBackground" />
        <attr name="text" />
    </declare-styleable>

    <declare-styleable name="ViewFinderOverlay">
        <attr name="strokeBoxWidth" format="float" />
        <attr name="strokeColor" format="color" />
        <attr name="backgroundRecticle" format="color" />
        <attr name="boxCornerRadius" format="float" />
        <attr name="desiredWidthCropPercent" format="integer" />
        <attr name="desiredHeightCropPercent" format="integer" />
    </declare-styleable>
</resources>